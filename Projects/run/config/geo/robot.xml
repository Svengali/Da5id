<geo>
	<m_texture			source="assets/DGLogo.png" />

	<m_pso source="embed" t="ResDGPipelineState">
		<m_vs source="assets/vehicle.vsh" />
		<m_ps source="assets/vehicle.psh" />
		<m_namedBuffers>
			<i>
				<m_name v="VehicleConstants"/>
				<m_buffer creator="ResDGBufferCreator">
					<size v="64" />
					<desc v="VehicleConstants" />
				</m_buffer>
			</i>
		</m_namedBuffers>
	</m_pso>

	<m_health max="50" />

	<m_vertexBuf source="+robot" creator="ResDGCubeCreator">
		<length v="0.5" />
		<width  v="1.0" />
		<height v="1.9" />
	</m_vertexBuf>
	<m_indexBuf source="+gen:0.5.indices"/>

</geo>